    },
    "./register": "./register/index.mjs"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "keywords": [
    "server",
    "express",
    "types-share",
    "fullstack"
  ],
  "author": "",
  "files": [
    "dist",
    "register",
    "bin"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/supertest": "^2.0.12",
    "express": "^4.18.2",
    "supertest": "^6.3.3",
    "tsup": "^6.5.0",
    "typescript": "^5.2.2",
    "vite": "^4.0.0"
  },
  "dependencies": {
    "@swc-node/core": "^1.10.6",
    "@swc-node/sourcemap-support": "^0.3.0",
    "@swc/core": "^1.3.51",
    "create-esm-loader": "^0.2.5",
    "es-module-lexer": "^1.4.1",
    "magic-string": "^0.30.5",
    "phecda-core": "workspace:*",
    "picocolors": "^1.0.0",
    "pirates": "^4.0.6",
    "reflect-metadata": "^0.1.13"
