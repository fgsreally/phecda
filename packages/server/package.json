{
  "name": "phecda-server",
  "version": "3.1.0",
  "description": "provide express middlewares, `nestjs` format",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "ps": "bin/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
