{
  "name": "phecda-client",
  "version": "3.0.0-alpha.0",
  "description": "client that connect http framework in phecda-server",
  "author": "fgsreally",
  "license": "MIT",
  "repository": "https://github.com/fgsreally/phecda/tree/main/packages/client",
  "keywords": [
    "phecda",
    "axios",
    "type-safe",
    "phecda-server"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./unplugin": {
      "require": "./dist/unplugin.js",
      "import": "./dist/unplugin.mjs"
    },
    "./http": {
      "require": "./dist/http/index.js",
      "import": "./dist/http/index.mjs"
    },
    "./rpc": {
      "require": "./dist/rpc/index.js",
      "import": "./dist/rpc/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "unplugin": [
        "dist/unplugin.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "unplugin": "^1.3.1"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "alova": "^3.2.7",
    "amqplib": "^0.10.3",
    "axios": "^1.3.5",
    "bullmq": "^5.7.1",
    "ioredis": "^5.3.2",
    "kafkajs": "^2.2.4",
    "nats": "^2.22.0",
    "phecda-server": "workspace:*",
    "tsup": "^8.1.0"
  }
}
